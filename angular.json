{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "angular-l10n": {
      "projectType": "library",
      "root": "projects/angular-l10n",
      "sourceRoot": "projects/angular-l10n/src",
      "prefix": "l10n",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/angular-l10n/tsconfig.lib.json",
            "project": "projects/angular-l10n/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/angular-l10n/tsconfig.lib.prod.json"
            },
            "development": {}
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/angular-l10n/src/test.ts",
            "tsConfig": "projects/angular-l10n/tsconfig.spec.json",
            "karmaConfig": "projects/angular-l10n/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/angular-l10n/**/*.ts",
              "projects/angular-l10n/**/*.html"
            ]
          }
        }
      }
    },
    "angular-l10n-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "standalone": true
        },
        "@schematics/angular:directive": {
          "standalone": true
        },
        "@schematics/angular:pipe": {
          "standalone": true
        }
      },
      "root": "projects/angular-l10n-app",
      "sourceRoot": "projects/angular-l10n-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/angular-l10n-app",
            "index": "projects/angular-l10n-app/src/index.html",
            "main": "projects/angular-l10n-app/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/angular-l10n-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/angular-l10n-app/src/favicon.ico",
              "projects/angular-l10n-app/src/assets"
            ],
            "styles": [
              "projects/angular-l10n-app/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "angular-l10n-app:build:production"
            },
            "development": {
              "browserTarget": "angular-l10n-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angular-l10n-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/angular-l10n-app/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/angular-l10n-app/src/favicon.ico",
              "projects/angular-l10n-app/src/assets"
            ],
            "styles": [
              "projects/angular-l10n-app/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/angular-l10n-app/**/*.ts",
              "projects/angular-l10n-app/**/*.html"
            ]
          }
        }
      }
    },
    "angular-l10n-ssr": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/angular-l10n-ssr",
      "sourceRoot": "projects/angular-l10n-ssr/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/angular-l10n-ssr/browser",
            "index": "projects/angular-l10n-ssr/src/index.html",
            "main": "projects/angular-l10n-ssr/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/angular-l10n-ssr/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/angular-l10n-ssr/src/favicon.ico",
              "projects/angular-l10n-ssr/src/assets"
            ],
            "styles": [
              "projects/angular-l10n-ssr/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "angular-l10n-ssr:build:production"
            },
            "development": {
              "browserTarget": "angular-l10n-ssr:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angular-l10n-ssr:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/angular-l10n-ssr/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/angular-l10n-ssr/src/favicon.ico",
              "projects/angular-l10n-ssr/src/assets"
            ],
            "styles": [
              "projects/angular-l10n-ssr/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/angular-l10n-ssr/**/*.ts",
              "projects/angular-l10n-ssr/**/*.html"
            ]
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/angular-l10n-ssr/server",
            "main": "projects/angular-l10n-ssr/server.ts",
            "tsConfig": "projects/angular-l10n-ssr/tsconfig.server.json",
            "inlineStyleLanguage": "scss"
          },
          "configurations": {
            "production": {
              "outputHashing": "media"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "sourceMap": true,
              "extractLicenses": false,
              "vendorChunk": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "configurations": {
            "development": {
              "browserTarget": "angular-l10n-ssr:build:development",
              "serverTarget": "angular-l10n-ssr:server:development"
            },
            "production": {
              "browserTarget": "angular-l10n-ssr:build:production",
              "serverTarget": "angular-l10n-ssr:server:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "routes": [
              "/"
            ]
          },
          "configurations": {
            "production": {
              "browserTarget": "angular-l10n-ssr:build:production",
              "serverTarget": "angular-l10n-ssr:server:production"
            },
            "development": {
              "browserTarget": "angular-l10n-ssr:build:development",
              "serverTarget": "angular-l10n-ssr:server:development"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    }
  },
  "cli": {
    "schematicCollections": [
      "@angular-eslint/schematics"
    ]
  },
  "schematics": {
    "@angular-eslint/schematics:application": {
      "setParserOptionsProject": true
    },
    "@angular-eslint/schematics:library": {
      "setParserOptionsProject": true
    }
  }
}
